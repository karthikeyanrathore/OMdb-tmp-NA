
>>> from core.models import Person, Movie, Playlist
>>> from core.db import db
>>> me = Person('admin','admin')
>>> p = Playlist('rap', True)
>>> me.playlists.append(p)
>>> m = Movie('ew3c')
>>> p.movies.append(m)
>>> db.session.add(me)
>>> db.session.commit()
